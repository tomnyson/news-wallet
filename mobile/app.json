{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
     "sdkVersion": "52.0.0",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/small-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#011359"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tomnyson.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tomnyson.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4cc3d8f4-77a1-43d0-b347-6e193791bcef"
      }
    }
  }
}
